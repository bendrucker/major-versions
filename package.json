  "author": "Ben Drucker <bvdrucker@gmail.com> (http://www.bendrucker.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bendrucker/major-versions/issues"
  },
  "homepage": "https://github.com/bendrucker/major-versions#readme",
